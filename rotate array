#include<bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t; 
	cin>>t;
	while(t)
	{
	    int n,d;
	    cin>>n>>d;
	    int arr[n];
	    vector<int>v;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    for(int i=d;i<n;i++)
	    {
	        v.push_back(arr[i]);
	    }
	    for(int i=0;i<d;i++)
	    {
	        v.push_back(arr[i]);
	    }
	    for(auto it:v)
	    {
	        cout<<it<<" ";
	    }
	    cout<<endl;
	    t--;
	    
	}
	
	return 0;
}
